{
	"name": "opensid-modules/template",
	"description": "Template untuk pengembangan Modul OpenSID",
	"license": "GPL-3.0-or-later",
	"type": "library",
	"keywords": [
		"template",
		"modules",
		"opensid"
	],
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"mikey179/vfsstream": "1.1.*"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"scripts": {
		"style-fix": [
			"npm run prettier-blade",
			"php-cs-fixer fix --allow-risky=yes",
			"rector process"
		]
	}
}
